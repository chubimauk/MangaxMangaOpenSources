{
  "name": "browserify-version",
  "version": "1.0.0",
  "description": "browserify for manga x manga",
  "main": "main.js",
  "scripts": {
  "test": "echo \"Error: no test specified\" && exit 1",
  "copy-root-to-src": "cp *.js src/",
  "babelize" : "npx babel src --out-dir src5",
  "copy-src5-to-root": "cp src5/* .",
  "bundle" : "browserify main.js --standalone myBundle > bundle.js",
  "copy-bundle": "cp bundle.js browserTest/",
  "restore-src": "cp src/* .",
  "build": "npm-run-all copy-root-to-src babelize copy-src5-to-root bundle copy-bundle restore-src"
},
  "author": "chubimauk",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "npm-run-all": "^4.1.5"
  }
}
